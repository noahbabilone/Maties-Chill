{% extends "MCBundle::admin.html.twig" %}  {% block stylesheets %}      {{ parent() }}      {% stylesheets      'https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/themes/smoothness/jquery-ui.css'      "bundles/mc/themes/global/plugins/bootstrap-datetimepicker/css/bootstrap-datetimepicker.min.css"      "bundles/mc/themes/global/plugins/bootstrap-select/css/bootstrap-select.min.css"      filter='cssrewrite' %}      <link rel="stylesheet" href="{{ asset_url }}"/>      {% endstylesheets %}      {# "bundles/mc/themes/global/plugins/bootstrap-datepicker/css/bootstrap-datepicker3.min.css"#}      <style type="text/css">          .ui-widget-content li {              font-size: 14px !important;              padding-left: 12px !important;              color: #555 !important;              background-color: #fff !important;          }          .ui-widget-content li:hover {              color: #fff !important;              background: #32c5d2 !important;          }      </style>  {% endblock %}{% block title %}    Dashboard - {{ parent() }}{% endblock %}{% block ad_body %}    <div class="page-bar">        <ul class="page-breadcrumb">            <li>                <a href="{{ path('home_page') }}"><i class="fa fa-home fa-fw"></i></a>                <i class="fa fa-circle"></i>            </li>            <li>                <span>Ajout Séance</span>            </li>        </ul>    </div>    <!-- END PAGE BAR -->    <!-- BEGIN PAGE TITLE-->    <h3 class="page-title"> Ajout d'une Séance</h3>    <!-- END PAGE TITLE-->    <!-- END PAGE HEADER-->    <div class="portlet light bordered">        <div class="portlet-title">            <div class="caption">                <span class="caption-subject font-red-sunglo bold uppercase"> <i class="icon-plus font-red-sunglo"></i> Formualire</span>                <span class="caption-helper">...</span>            </div>            <div class="tools">                <a href="javascript:;" class="collapse"> </a>                <a href="#portlet-config" data-toggle="modal" class="config"> </a>                <a href="javascript:;" class="reload"> </a>                <a href="" class="fullscreen"> </a>                <a href="javascript:;" class="remove"> </a>            </div>        </div>        <div class="portlet-body form">            <!-- BEGIN FORM-->            <div class="row">                <div class="col-sm-12">                    <div class="alert alert-warning hide" id="field-message-error" role="alert">                        {#<a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>#}                        <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>                        <span class="sr-only">Error:</span>                        <span class="message-error"></span>                    </div>                    {% if result is defined %}                        {% if result == "success" %}                            <div class="alert alert-success" role="alert">                                <button type="button" class="close" data-dismiss="alert"                                        aria-hidden="true"></button>                                <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>                                L'utulisateur a été ajouté avec succès .                            </div>                        {% else %}                            <div class="alert alert-warning" role="alert">                                <button type="button" class="close" data-dismiss="alert"                                        aria-hidden="true"></button>                                <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>                                <span class="sr-only">Error:</span>                                {{ result }}                            </div>                        {% endif %}                        {% if error is defined and error %}                            <div class="alert alert-danger" role="alert" style="margin-top: 30px;">                                <button type="button" class="close" data-dismiss="alert"                                        aria-hidden="true"></button>                                <span class="glyphicon glyphicon-warning-sign" aria-hidden="true"></span>                                {{ error | raw }}.                            </div>                        {% endif %}                    {% endif %}                </div>                {% if form is defined %}                    {{ form_start(form, { 'method': 'POST','attr': { 'role':'form'} }) }}                    <div class="col-sm-6">                        <div class="form-body">                            <div class="row">                                {% if form.filmId is defined %}                                    <div class="col-sm-12">                                        <div class="form-group has-feedback" id="field-name">                                            {{ form_label(form.filmId) }}                                            <div class="input-icon right">                                                <i class="fa fa-film" id="icon_search_film"></i>                                                <input type="text" class="form-control" id="search_film"                                                       placeholder="Rechercher" autocomplete="off" required/>                                            </div>                                        </div>                                        {#<input type="text" id="id_film" autocomplete="off">#}                                        <div class="hide" style="display: none;">                                            film {{ form_widget(form.filmId) }}                                        </div>                                    </div>                                {% endif %}                                {% if form.date is defined %}                                    <div class="col-md-6">                                        {#<div class="form-group has-feedback" id="field-name">#}                                        {{ form_label(form.date) }}                                        <div class="input-group date form_datetime">                                            <input type="text" class="form-control" id="input_date_session"                                                   autocomplete="off" value="" required/>                                            <span class="input-group-btn">                                                <button class="btn default date-set" type="button">                                                    <i class="fa fa-calendar"></i>                                                    </button>                                                </span>                                        </div>                                        <div class="hide" style="display: none;">                                            {{ form_widget(form.date) }}                                        </div>                                        {#</div>#}                                    </div>                                {% endif %}                                {% if form.typeView is defined %}                                    <div class="col-sm-6">                                        <div class="form-group has-feedback" id="#">                                            {{ form_label(form.typeView) }}                                            <div class="input-icon">                                                <i class="fa fa-video-camera"></i>                                                {{ form_widget(form.typeView) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                {% if form.modality is defined %}                                    <div class="col-sm-6">                                        <div class="form-group has-feedback" id="#">                                            {{ form_label(form.modality) }}                                            <div class="input-icon">                                                <i class="fa fa-money"></i>                                                {{ form_widget(form.modality) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                {% if form.maxPlace is defined %}                                    <div class="col-sm-6">                                        <div class="form-group has-feedback" id="#">                                            {{ form_label(form.maxPlace) }}                                            <div class="input-icon">                                                <i class="fa fa-group"></i>                                                {{ form_widget(form.maxPlace) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                {% if form.price is defined %}                                    <div class="col-sm-6 hide" id="input_price">                                        <div class="form-group has-feedback">                                            {{ form_label(form.price) }}                                            <div class="input-icon">                                                <i class="fa fa-eur"></i>                                                {{ form_widget(form.price) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                                {% if form.contribution is defined %}                                    <div class="col-sm-12 hide" id="input_contribution">                                        <div class="form-group has-feedback" id="#">                                            {{ form_label(form.contribution) }}                                            <div class="input-icon">                                                <i class="fa fa-bank"></i>                                                {{ form_widget(form.contribution) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                            </div>                        </div>                        <!-- END FORM-->                    </div>                    <div class="col-sm-6">                        <div class="form-body">                            <div class="row">                                {% if form.material is defined %}                                    <div class="col-sm-12">                                        {{ form_label(form.material) }}                                    </div>                                    <div class="col-sm-8">                                        <div class="form-group has-feedback">                                            {{ form_widget(form.material, {'attr': {'class': 'bs-select form-control'}}) }}                                        </div>                                    </div>                                    <div class="col-sm-4">                                        <a class="btn green btn-block"                                           data-toggle="modal" href="#btn_add_material"><i                                                    class="fa fa-plus fa-fw"></i>Ajouter un material                                        </a>                                    </div>                                {% endif %}                                {% if form.address is defined %}                                    <div class="col-sm-12">                                        {{ form_label(form.address) }}                                    </div>                                    <div class="col-sm-8">                                        <div class="form-group has-feedback">                                            <div class="input-icon">                                                <i class="fa fa-building"></i>                                                {{ form_widget(form.address) }}                                            </div>                                        </div>                                    </div>                                    <div class="col-sm-4">                                        <a class="btn green btn-block"                                           data-toggle="modal" href="#btn_add_address"><i                                                    class="fa fa-plus fa-fw"> </i>Ajouter une adresse                                        </a>                                    </div>                                {% endif %}                                <div class="col-sm-12">                                    <hr>                                </div>                                {% if form.description is defined %}                                    <div class="col-sm-12">                                        <div class="form-group has-feedback">                                            {{ form_label(form.description) }}                                            <div class="input-icon">                                                <i class="fa fa-list-alt"></i>                                                {{ form_widget(form.description) }}                                            </div>                                        </div>                                    </div>                                {% endif %}                            </div>                        </div>                        <!-- END FORM-->                    </div>                    <div class="col-sm-12">                        <div class="form-actions">                            <div class="row">                                <div class="col-md-3 col-offset-sm-3">                                    <button type="submit" class="btn green btn-block btn-add-user">Envoyer</button>                                </div>                                <div class="col-md-3">                                    <button type="button" class="btn default btn-block">Annuler</button>                                </div>                            </div>                        </div>                    </div>                    {{ form_end(form) }}                {% endif %}            </div>        </div>    </div>    <div id="btn_add_material" class="modal fade" tabindex="-1" data-backdrop="static" data-keyboard="false">        <div class="modal-dialog">            <div class="modal-content">                <div class="modal-header">                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>                    <h4 class="modal-title">Ajout un material</h4>                </div>                <div class="modal-body">                    {% if form is defined %}                        {{ form_start(formMaterial, { 'method': 'POST','attr': { 'role':'form'} }) }}                        <div class="row">                            {% if formMaterial.title is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formMaterial.title) }}                                        <div class="input-icon">                                            <i class="fa fa-home"></i>                                            {{ form_widget(formMaterial.title) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formMaterial.typeMaterial is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formMaterial.typeMaterial) }}                                        <div class="input-icon">                                            <i class="fa fa-tv"></i>                                            {{ form_widget(formMaterial.typeMaterial) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formMaterial.description is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formMaterial.description) }}                                        <div class="input-icon">                                            <i class="fa fa-list-alt"></i>                                            {{ form_widget(formMaterial.description) }}                                        </div>                                    </div>                                </div>                            {% endif %}                        </div>                        {{ form_end(formMaterial) }}                    {% endif %}                </div>                <div class="modal-footer">                    <button type="button" data-dismiss="modal" class="btn dark btn-outline">Annuler <i                                class="fa fa-remove"></i></button>                    <button type="button" data-dismiss="modal" class="btn green">Enregistrer <i class="fa fa-check"></i>                    </button>                </div>            </div>        </div>    </div>    <div id="btn_add_address" class="modal fade" tabindex="-1" data-backdrop="static" data-keyboard="false">        <div class="modal-dialog">            <div class="modal-content">                <div class="modal-header">                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>                    <h4 class="modal-title">Ajout une adresse</h4>                </div>                <div class="modal-body">                    {% if form is defined %}                        {{ form_start(formAddress, { 'method': 'POST','attr': { 'role':'form'} }) }}                        <div class="row">                            {% if formAddress.title is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.title) }}                                        <div class="input-icon">                                            <i class="fa fa-home"></i>                                            {{ form_widget(formAddress.title) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.street is defined %}                                <div class="col-sm-2">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.street) }}                                        <div class="input-icon">                                            <i class="fa fa-road"></i>                                            {{ form_widget(formAddress.street) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.address is defined %}                                <div class="col-sm-10">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.address) }}                                        <div class="input-icon">                                            <i class="fa fa-map-marker"></i>                                            {{ form_widget(formAddress.address) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.address2 is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.address2) }}                                        <div class="input-icon">                                            <i class="fa fa-list-alt"></i>                                            {{ form_widget(formAddress.address2) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.postCode is defined %}                                <div class="col-sm-5">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.postCode) }}                                        <div class="input-icon">                                            <i class="fa fa-globe"></i>                                            {{ form_widget(formAddress.postCode) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.town is defined %}                                <div class="col-sm-7">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.town) }}                                        <div class="input-icon">                                            <i class="fa fa-location-arrow "></i>                                            {{ form_widget(formAddress.town) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.frontDoor is defined %}                                <div class="col-sm-4">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.frontDoor) }}                                        <div class="input-icon">                                            <i class="fa fa-sign-in"></i>                                            {{ form_widget(formAddress.frontDoor) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.floor is defined %}                                <div class="col-sm-4">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.floor) }}                                        <div class="input-icon">                                            <i class="fa fa-building"></i>                                            {{ form_widget(formAddress.floor) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.building is defined %}                                <div class="col-sm-4">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.building) }}                                        <div class="input-icon">                                            <i class="fa fa-building-o"></i>                                            {{ form_widget(formAddress.building) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.phone is defined %}                                <div class="col-sm-6">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.phone) }}                                        <div class="input-icon">                                            <i class="fa fa-phone"></i>                                            {{ form_widget(formAddress.phone) }}                                        </div>                                    </div>                                </div>                            {% endif %}                        </div>                        {{ form_end(formAddress) }}                    {% endif %}                </div>                <div class="modal-footer">                    <button type="button" data-dismiss="modal" class="btn dark btn-outline">Annuler <i                                class="fa fa-remove"></i></button>                    <button type="button" data-dismiss="modal" class="btn green">Enregistrer <i class="fa fa-check"></i>                    </button>                </div>            </div>        </div>    </div>{% endblock %}    {% block javascripts %}        {{ parent() }}        {% javascripts        "https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"        "bundles/mc/themes/global/plugins/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js"        "bundles/mc/themes/global/plugins/bootstrap-select/js/bootstrap-select.min.js"        "bundles/mc/js/form.js" %}        <script src="{{ asset_url }}"></script>        {% endjavascripts %}        <script type="text/javascript">            $(function () {                $(".form_datetime").datetimepicker({                    autoclose: true,                    isRTL: App.isRTL(),                    format: "dd MM yyyy hh:ii",                    pickerPosition: (App.isRTL() ? "bottom-right" : "bottom-left")                });            });            $('.bs-select').selectpicker({                iconBase: 'fa',                tickIcon: 'fa-check',                noneSelectedText: "Choisir materiel(s)"            });            $(".btn-remove").click(function (e) {                e.preventDefault();            });        </script>    {% endblock %}