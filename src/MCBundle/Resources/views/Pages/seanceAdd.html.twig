{% extends "MCBundle::main.html.twig" %}{% block title %}    Ajout film - {{ parent() }}{% endblock %}{% block stylesheets %}    {{ parent() }}    {% stylesheets    'https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/themes/smoothness/jquery-ui.css'    "bundles/mc/themes/global/plugins/bootstrap-datetimepicker/css/bootstrap-datetimepicker.min.css"    "bundles/mc/themes/global/plugins/bootstrap-select/css/bootstrap-select.min.css"    filter='cssrewrite' %}    <link rel="stylesheet" href="{{ asset_url }}"/>    {% endstylesheets %}    <style type="text/css">        .ui-widget-content li {            font-size: 14px !important;            padding-left: 12px !important;            color: #555 !important;            background-color: #fff !important;        }        .ui-widget-content li:hover {            color: #fff !important;            background: #32c5d2 !important;        }        .autocomplete-image {            width: 40px;            height: 41px;            padding-right: 10px;        }        .ui-autocomplete > li {            /*height: 40px;*/        }    </style>{% endblock %}{% block body_mc %}    <div class="page-container page-mc">        <div class="page-content">            <div class="page-bar">                <div class="row">                    <div class="col-sm-12">                        <ul class="page-breadcrumb">                            <li>                                <a href="{{ path('home_page') }}">Accueil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <a href="{{ path('film_home') }}">Films</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <span>Ajout</span>                            </li>                        </ul>                    </div>                </div>            </div>            <!-- END PAGE BAR -->            <!-- BEGIN PAGE TITLE-->            <div class="row">                <div class="col-sm-2">                    {% include("MCBundle:Includes:submenu.html.twig") %}                </div>                <div class="col-sm-10">                    <div class="row">                        <div class="col-sm-12">                            <h3 class="page-title page-header">Séance                                <small>Ajout</small>                            </h3>                        </div>                        {% if form is defined %}                            {{ form_start(form, { 'method': 'POST','attr': { 'role':'form'} }) }}                            <div class="col-sm-6 session-form">                                <div class="form-body">                                    <div class="row">                                        {% if form.filmId is defined %}                                            <div class="col-sm-12">                                                {{ form_label(form.filmId) }}                                            </div>                                            <div class="col-sm-12">                                                <div class="form-group has-feedback" id="field-name">                                                    <div class="input-group">                                                        <div class="input-icon right">                                                            <i class="fa fa-film fa-fw" id="icon_search_film"></i>                                                            <input type="text" class="form-control" id="search_film"                                                                   placeholder="Rechercher" autocomplete="off"                                                                   required/>                                                        </div>                                                    <span class="input-group-btn">                                                        <button data-toggle="modal" id="btn_search_film"                                                                class="btn green" type="button" disabled>                                                            <i class="fa fa-search fa-fw" id="btn_icon_search"></i>                                                        </button>                                                    </span>                                                    </div>                                                </div>                                            </div>                                            <div class="hide" style="display: none;">                                                film {{ form_widget(form.filmId) }}                                            </div>                                        {% endif %}                                        {% if form.date is defined %}                                            <div class="col-sm-6">                                                {#<div class="form-group has-feedback" id="field-name">#}                                                {{ form_label(form.date) }}                                                <div class="input-group date form_datetime">                                                    <input type="text" class="form-control" id="input_date_session"                                                           autocomplete="off" value="" required/>                                            <span class="input-group-btn">                                                <button class="btn default date-set" type="button">                                                    <i class="fa fa-calendar"></i>                                                    </button>                                                </span>                                                </div>                                                <div class="hide" style="display: none;">                                                    {{ form_widget(form.date) }}                                                </div>                                                {#</div>#}                                            </div>                                        {% endif %}                                        {% if form.typeView is defined %}                                            <div class="col-sm-6">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.typeView) }}                                                    <div class="input-icon">                                                        <i class="fa fa-video-camera"></i>                                                        {{ form_widget(form.typeView) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.modality is defined %}                                            <div class="col-sm-6">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.modality) }}                                                    <div class="input-icon">                                                        <i class="fa fa-money"></i>                                                        {{ form_widget(form.modality) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.maxPlace is defined %}                                            <div class="col-sm-6">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.maxPlace) }}                                                    <div class="input-icon">                                                        <i class="fa fa-group"></i>                                                        {{ form_widget(form.maxPlace) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.price is defined %}                                            <div class="col-sm-6 hide" id="input_price">                                                <div class="form-group has-feedback">                                                    {{ form_label(form.price) }}                                                    <div class="input-icon">                                                        <i class="fa fa-eur"></i>                                                        {{ form_widget(form.price) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.contribution is defined %}                                            <div class="col-sm-12 hide" id="input_contribution">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.contribution) }}                                                    <div class="input-icon">                                                        <i class="fa fa-bank"></i>                                                        {{ form_widget(form.contribution) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.material is defined %}                                            <div class="col-sm-12">                                                {{ form_label(form.material) }}                                            </div>                                            <div class="col-sm-8">                                                <div class="form-group has-feedback">                                                    {{ form_widget(form.material, {'attr': {'class': 'bs-select form-control'}}) }}                                                </div>                                            </div>                                            <div class="col-sm-4">                                                <a class="btn green btn-block"                                                   data-toggle="modal" href="#btn_add_material"><i                                                            class="fa fa-plus fa-fw"></i>Ajouter un material                                                </a>                                            </div>                                        {% endif %}                                        {% if form.address is defined %}                                            <div class="col-sm-12">                                                {{ form_label(form.address) }}                                            </div>                                            <div class="col-sm-8">                                                <div class="form-group has-feedback">                                                    <div class="input-icon">                                                        <i class="fa fa-building"></i>                                                        {{ form_widget(form.address) }}                                                    </div>                                                </div>                                            </div>                                            <div class="col-sm-4">                                                <a class="btn green btn-block"                                                   data-toggle="modal" href="#btn_add_address"><i                                                            class="fa fa-plus fa-fw"> </i>Ajouter une adresse                                                </a>                                            </div>                                        {% endif %}                                        <div class="col-sm-12">                                            <hr>                                        </div>                                        {% if form.description is defined %}                                            <div class="col-sm-12">                                                <div class="form-group has-feedback">                                                    {{ form_label(form.description) }}                                                    <div class="input-icon">                                                        <i class="fa fa-list-alt"></i>                                                        {{ form_widget(form.description) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                    </div>                                </div>                            </div>                            <div class="col-sm-6 session-form">                                <div class="form-body">                                    <div class="row">                                        <div class="col-sm-12">                                            <div class="row">                                                <div class="col-sm-12" >                                                    <label class="font-green bold" style="padding-left: 17px;">Film: <span                                                                class="film_title"></span></label>                                                </div>                                                <div class="col-sm-3">                                                    <img src="{{ asset('assets/img/default.png') }}" alt=""                                                         id="film_poster"                                                         class="pull-left"                                                         style=" width: 130px; height: 190px; margin-bottom: 10px;"/>                                                    <button class="btn green btn btn-outline hide" id="btn_allocine">Liens Allocine                                                    </button>                                                </div>                                                <div class="col-sm-9">                                                    <table class="table table-hover table-light"                                                           id="film_info">                                                        <tr class="tr_film_directors hide">                                                            <td>Porducteur:</td>                                                            <td class="film_directors"></td>                                                        </tr>                                                        <tr class="tr_film_year hide">                                                            <td>Années:</td>                                                            <td class="film_year"></td>                                                        </tr>                                                        <tr class="tr_film_actors hide">                                                            <td>Acteurs:</td>                                                            <td class="film_actors"></td>                                                        </tr>                                                        <tr class="tr_film_synopsis hide">                                                            <td>Synopsis:</td>                                                            <td class="film_synopsis"></td>                                                        </tr>                                                    </table>                                                    <img src="{{ asset('assets/img/default.png') }}" alt=""                                                         class="hide" style="display: none;" id="film_poster_default">                                                </div>                                            </div>                                        </div>                                    </div>                                </div>                                <!-- END FORM-->                            </div>                            <div class="col-sm-12"></div>                            <div class="form-actions">                                <div class="row">                                    <div class="col-sm-3 col-offset-sm-3">                                        <button type="submit" class="btn green btn-block btn-add-user">Envoyer</button>                                    </div>                                    <div class="col-sm-3">                                        <button type="button" class="btn default btn-block">Annuler</button>                                    </div>                                </div>                            </div>                            {{ form_end(form) }}                        {% endif %}                    </div>                </div>            </div>        </div>    </div>    <div id="btn_add_material" class="modal fade" tabindex="-1" data-backdrop="static" data-keyboard="false">        <div class="modal-dialog">            <div class="modal-content">                <div class="modal-header">                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>                    <h4 class="modal-title">Ajout un material</h4>                </div>                <div class="modal-body">                    {% if formMaterial is defined %}                        {{ form_start(formMaterial, { 'method': 'POST','attr': { 'role':'form'} }) }}                        <div class="row">                            {% if formMaterial.title is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formMaterial.title) }}                                        <div class="input-icon">                                            <i class="fa fa-american-sign-language-interpreting"></i>                                            {{ form_widget(formMaterial.title) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formMaterial.typeMaterial is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formMaterial.typeMaterial) }}                                        <div class="input-icon">                                            <i class="fa fa-tv"></i>                                            {{ form_widget(formMaterial.typeMaterial) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formMaterial.description is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formMaterial.description) }}                                        <div class="input-icon">                                            <i class="fa fa-list-alt"></i>                                            {{ form_widget(formMaterial.description) }}                                        </div>                                    </div>                                </div>                            {% endif %}                        </div>                        {{ form_end(formMaterial) }}                    {% endif %}                </div>                <div class="modal-footer">                    <button type="button" data-dismiss="modal" class="btn dark btn-outline">Annuler <i                                class="fa fa-remove"></i></button>                    <button type="button" data-dismiss="modal" class="btn green">Enregistrer <i class="fa fa-check"></i>                    </button>                </div>            </div>        </div>    </div>    <div id="btn_add_address" class="modal fade" tabindex="-1" data-backdrop="static" data-keyboard="false">        <div class="modal-dialog">            <div class="modal-content">                <div class="modal-header">                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>                    <h4 class="modal-title">Ajout une adresse</h4>                </div>                <div class="modal-body">                    {% if formAddress is defined %}                        {{ form_start(formAddress, { 'method': 'POST','attr': { 'role':'form'} }) }}                        <div class="row">                            {% if formAddress.title is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.title) }}                                        <div class="input-icon">                                            <i class="fa fa-home"></i>                                            {{ form_widget(formAddress.title) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.street is defined %}                                <div class="col-sm-2">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.street) }}                                        <div class="input-icon">                                            <i class="fa fa-road"></i>                                            {{ form_widget(formAddress.street) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.address is defined %}                                <div class="col-sm-10">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.address) }}                                        <div class="input-icon">                                            <i class="fa fa-map-marker"></i>                                            {{ form_widget(formAddress.address) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.address2 is defined %}                                <div class="col-sm-12">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.address2) }}                                        <div class="input-icon">                                            <i class="fa fa-list-alt"></i>                                            {{ form_widget(formAddress.address2) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.postCode is defined %}                                <div class="col-sm-5">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.postCode) }}                                        <div class="input-icon">                                            <i class="fa fa-globe"></i>                                            {{ form_widget(formAddress.postCode) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.town is defined %}                                <div class="col-sm-7">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.town) }}                                        <div class="input-icon">                                            <i class="fa fa-location-arrow "></i>                                            {{ form_widget(formAddress.town) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.frontDoor is defined %}                                <div class="col-sm-4">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.frontDoor) }}                                        <div class="input-icon">                                            <i class="fa fa-sign-in"></i>                                            {{ form_widget(formAddress.frontDoor) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.floor is defined %}                                <div class="col-sm-4">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.floor) }}                                        <div class="input-icon">                                            <i class="fa fa-building"></i>                                            {{ form_widget(formAddress.floor) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.building is defined %}                                <div class="col-sm-4">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.building) }}                                        <div class="input-icon">                                            <i class="fa fa-building-o"></i>                                            {{ form_widget(formAddress.building) }}                                        </div>                                    </div>                                </div>                            {% endif %}                            {% if formAddress.phone is defined %}                                <div class="col-sm-6">                                    <div class="form-group has-feedback" id="#">                                        {{ form_label(formAddress.phone) }}                                        <div class="input-icon">                                            <i class="fa fa-phone"></i>                                            {{ form_widget(formAddress.phone) }}                                        </div>                                    </div>                                </div>                            {% endif %}                        </div>                        {{ form_end(formAddress) }}                    {% endif %}                </div>                <div class="modal-footer">                    <button type="button" data-dismiss="modal" class="btn dark btn-outline">Annuler <i                                class="fa fa-remove"></i></button>                    <button type="button" data-dismiss="modal" class="btn green">Enregistrer <i class="fa fa-check"></i>                    </button>                </div>            </div>        </div>    </div>    <div id="modal_search_film" class="modal fade bs-modal-lg"         tabindex="-1" data-backdrop="static" data-keyboard="false">        <div class="modal-dialog modal-lg">            <div class="modal-content">                <div class="modal-header">                    {#<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>#}                    {#<h4 class="modal-title">Rechercher</h4>#}                    {#<div class="col-sm-12">#}                    <div class="form-group has-feedback" id="field-name">                        <div class="input-group">                            <div class="input-icon right">                                <i class="fa fa-film fa-fw" id="icon_search_film"></i>                                <input type="text" class="form-control" id="modal_input_search_film"                                       placeholder="Rechercher" autocomplete="off"                                       required/>                            </div>                                    <span class="input-group-btn">                                        <button data-toggle="modal" id="btn_modal_search_film"                                                class="btn green" type="button">                                            <i class="fa fa-search fa-fw"></i> Rechercher                                        </button>                                        <button data-toggle="modal" id="btn_modal_search_film"                                                class="btn red" type="button"                                                data-dismiss="modal" aria-hidden="true">                                            <i class="fa fa-remove fa-fw"></i> Fermer                                        </button>                                    </span>                        </div>                    </div>                    {#</div>#}                </div>                <div class="modal-body" id="result_film"></div>                <div class="modal-footer">                    <button type="button" data-dismiss="modal" class="btn dark btn-outline">Fermer</button>                </div>            </div>        </div>    </div>    {% endblock %}   {% block javascripts %}       {{ parent() }}       {% javascripts       "https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"       "bundles/mc/themes/global/plugins/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js"       "bundles/mc/themes/global/plugins/bootstrap-select/js/bootstrap-select.min.js"       "bundles/mc/js/form.js" %}       <script src="{{ asset_url }}"></script>       {% endjavascripts %}       <script type="text/javascript">           $(function () {               $(".form_datetime").datetimepicker({                   autoclose: true,                   isRTL: App.isRTL(),                   format: "dd MM yyyy hh:ii",                   pickerPosition: (App.isRTL() ? "bottom-right" : "bottom-left")               });           });           $('.bs-select').selectpicker({               iconBase: 'fa',               tickIcon: 'fa-check',               noneSelectedText: "Choisir materiel(s)"           });           $(".btn-remove").click(function (e) {               e.preventDefault();           });       </script>   {% endblock %}