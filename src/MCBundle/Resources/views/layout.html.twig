{% extends "::base.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
    'bundles/mc/bootstrap/css/bootstrap-theme.css'
    'https://fonts.googleapis.com/icon?family=Material+Icons'
    'bundles/mc/css/style.css'
    'bundles/mc/css/heroic-features.css'
    filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}


{% block body %}

    {#{% include("MCBundle:Includes:menu.html.twig") %}#}

    <header class="sb-page-header">
    </header>
    <div class="container">
        {% block body_mc %}
        {% endblock %}
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    <hr>
                    <p>Copyright &copy; <a href="">Estiam Groupe-4 </a>. 2016</p>
                </div>
            </div>
        </footer>
    </div>

{% endblock %}


{% block javascripts %}
    {{ parent() }}
    {% javascripts
    'https://code.getmdl.io/1.1.3/material.min.js'
    'bundles/mc/js/app.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

