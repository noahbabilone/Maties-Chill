{% extends "MCBundle::main.html.twig" %}{% block title %}    Ajout film - {{ parent() }}{% endblock %}{% block stylesheets %}    {{ parent() }}    {% stylesheets    'https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/themes/smoothness/jquery-ui.css'    "bundles/mc/themes/global/plugins/bootstrap-datetimepicker/css/bootstrap-datetimepicker.min.css"    "bundles/mc/themes/global/plugins/bootstrap-select/css/bootstrap-select.min.css"    filter='cssrewrite' %}    {#           '#}    <link rel="stylesheet" href="{{ asset_url }}"/>    {% endstylesheets %}    <style type="text/css">        .ui-widget-content li {            font-size: 14px !important;            padding-left: 12px !important;            color: #555 !important;            background-color: #fff !important;        }        .ui-widget-content li:hover {            color: #fff !important;            background: #32c5d2 !important;        }    </style>{% endblock %}{% block body_mc %}    <div class="page-container page-mc">        <div class="page-content">            <div class="page-bar">                <div class="row">                    <div class="col-sm-12">                        <ul class="page-breadcrumb">                            <li>                                <a href="{{ path('home_page') }}">Accueil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <a href="{{ path('film_home') }}">Films</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <span>Ajout</span>                            </li>                        </ul>                    </div>                </div>            </div>            <!-- END PAGE BAR -->            <!-- BEGIN PAGE TITLE-->            <div class="row">                <div class="col-sm-2">                    {% include("MCBundle:Includes:submenu.html.twig") %}                </div>                <div class="col-sm-10">                    <div class="row">                        <div class="col-sm-12">                            <h3 class="page-title page-header">SÃ©ance                                <small>Ajout</small>                            </h3>                        </div>                        <div class="col-sm-6 session-form">                            <div class="form-body">                                <div class="row">                                    {% if form.filmId is defined %}                                        <div class="col-sm-12">                                            <div class="form-group has-feedback" id="field-name">                                                {{ form_label(form.filmId) }}                                                <div class="input-icon right">                                                    <i class="fa fa-film" id="icon_search_film"></i>                                                    <input type="text" class="form-control" id="search_film"                                                           placeholder="Rechercher" autocomplete="off" required/>                                                </div>                                            </div>                                            {#<input type="text" id="id_film" autocomplete="off">#}                                            <div class="hide" style="display: none;">                                                film {{ form_widget(form.filmId) }}                                            </div>                                        </div>                                    {% endif %}                                    {% if form.date is defined %}                                        <div class="col-sm-6">                                            {#<div class="form-group has-feedback" id="field-name">#}                                            {{ form_label(form.date) }}                                            <div class="input-group date form_datetime">                                                <input type="text" class="form-control" id="input_date_session"                                                       autocomplete="off" value="" required/>                                            <span class="input-group-btn">                                                <button class="btn default date-set" type="button">                                                    <i class="fa fa-calendar"></i>                                                    </button>                                                </span>                                            </div>                                            <div class="hide" style="display: none;">                                                {{ form_widget(form.date) }}                                            </div>                                            {#</div>#}                                        </div>                                    {% endif %}                                    {% if form.typeView is defined %}                                        <div class="col-sm-6">                                            <div class="form-group has-feedback" id="#">                                                {{ form_label(form.typeView) }}                                                <div class="input-icon">                                                    <i class="fa fa-video-camera"></i>                                                    {{ form_widget(form.typeView) }}                                                </div>                                            </div>                                        </div>                                    {% endif %}                                    {% if form.modality is defined %}                                        <div class="col-sm-6">                                            <div class="form-group has-feedback" id="#">                                                {{ form_label(form.modality) }}                                                <div class="input-icon">                                                    <i class="fa fa-money"></i>                                                    {{ form_widget(form.modality) }}                                                </div>                                            </div>                                        </div>                                    {% endif %}                                    {% if form.maxPlace is defined %}                                        <div class="col-sm-6">                                            <div class="form-group has-feedback" id="#">                                                {{ form_label(form.maxPlace) }}                                                <div class="input-icon">                                                    <i class="fa fa-group"></i>                                                    {{ form_widget(form.maxPlace) }}                                                </div>                                            </div>                                        </div>                                    {% endif %}                                    {% if form.price is defined %}                                        <div class="col-sm-6 hide" id="input_price">                                            <div class="form-group has-feedback">                                                {{ form_label(form.price) }}                                                <div class="input-icon">                                                    <i class="fa fa-eur"></i>                                                    {{ form_widget(form.price) }}                                                </div>                                            </div>                                        </div>                                    {% endif %}                                    {% if form.contribution is defined %}                                        <div class="col-sm-12 hide" id="input_contribution">                                            <div class="form-group has-feedback" id="#">                                                {{ form_label(form.contribution) }}                                                <div class="input-icon">                                                    <i class="fa fa-bank"></i>                                                    {{ form_widget(form.contribution) }}                                                </div>                                            </div>                                        </div>                                    {% endif %}                                </div>                            </div>                        </div>                        <div class="col-sm-6 session-form">                            <div class="form-body">                                <div class="row">                                    {% if form.material is defined %}                                        <div class="col-sm-12">                                            {{ form_label(form.material) }}                                        </div>                                        <div class="col-sm-8">                                            <div class="form-group has-feedback">                                                {{ form_widget(form.material, {'attr': {'class': 'bs-select form-control'}}) }}                                            </div>                                        </div>                                        <div class="col-sm-4">                                            <a class="btn green btn-block"                                               data-toggle="modal" href="#btn_add_material"><i                                                        class="fa fa-plus fa-fw"></i>Ajouter un material                                            </a>                                        </div>                                    {% endif %}                                    {% if form.address is defined %}                                        <div class="col-sm-12">                                            {{ form_label(form.address) }}                                        </div>                                        <div class="col-sm-8">                                            <div class="form-group has-feedback">                                                <div class="input-icon">                                                    <i class="fa fa-building"></i>                                                    {{ form_widget(form.address) }}                                                </div>                                            </div>                                        </div>                                        <div class="col-sm-4">                                            <a class="btn green btn-block"                                               data-toggle="modal" href="#btn_add_address"><i                                                        class="fa fa-plus fa-fw"> </i>Ajouter une adresse                                            </a>                                        </div>                                    {% endif %}                                    <div class="col-sm-12">                                        <hr>                                    </div>                                    {% if form.description is defined %}                                        <div class="col-sm-12">                                            <div class="form-group has-feedback">                                                {{ form_label(form.description) }}                                                <div class="input-icon">                                                    <i class="fa fa-list-alt"></i>                                                    {{ form_widget(form.description) }}                                                </div>                                            </div>                                        </div>                                    {% endif %}                                </div>                            </div>                            <!-- END FORM-->                        </div>                        <div class="col-sm-12"></div>                        <div class="form-actions">                            <div class="row">                                <div class="col-sm-3 col-offset-sm-3">                                    <button type="submit" class="btn green btn-block btn-add-user">Envoyer</button>                                </div>                                <div class="col-sm-3">                                    <button type="button" class="btn default btn-block">Annuler</button>                                </div>                            </div>                        </div>                    </div>                </div>            </div>        </div>    </div>{% endblock %}   {% block javascripts %}       {{ parent() }}       {% javascripts       "https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"       "bundles/mc/themes/global/plugins/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js"       "bundles/mc/themes/global/plugins/bootstrap-select/js/bootstrap-select.min.js"       "bundles/mc/js/form.js" %}       <script src="{{ asset_url }}"></script>       {% endjavascripts %}       <script type="text/javascript">           $(function () {               $(".form_datetime").datetimepicker({                   autoclose: true,                   isRTL: App.isRTL(),                   format: "dd MM yyyy hh:ii",                   pickerPosition: (App.isRTL() ? "bottom-right" : "bottom-left")               });           });           $('.bs-select').selectpicker({               iconBase: 'fa',               tickIcon: 'fa-check',               noneSelectedText: "Choisir materiel(s)"           });           $(".btn-remove").click(function (e) {               e.preventDefault();           });       </script>   {% endblock %}