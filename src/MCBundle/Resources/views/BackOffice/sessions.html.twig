{% extends "MCBundle::admin.html.twig" %}  {% block stylesheets %}      {{ parent() }}      {% stylesheets      filter='cssrewrite' %}      <link rel="stylesheet" href="{{ asset_url }}"/>      {% endstylesheets %}  {% endblock %}{% block title %}    Dashboard - {{ parent() }}{% endblock %}{% block ad_body %}    <!-- BEGIN PAGE HEADER-->    <!-- BEGIN PAGE BAR -->    <div class="page-bar">        <ul class="page-breadcrumb">            <li>                <a href="{{ path('dashboard') }}"><i class="fa fa-home fa-fw"></i></a>                <i class="fa fa-circle"></i>            </li>            <li>                <span>Listes Séances</span>            </li>        </ul>    </div>    <!-- END PAGE BAR -->    <!-- BEGIN PAGE TITLE-->    <h3 class="page-title">Séances        <small>Listes</small>    </h3>    <!-- END PAGE TITLE-->    <!-- END PAGE HEADER-->    <div class="row">        <div class="col-md-12">            <!-- BEGIN SAMPLE TABLE PORTLET-->            <div class="portlet light bordered">                <div class="portlet-title">                    <div class="caption">                        <i class="fa fa-list fa-fw font-green"></i>                        <span class="caption-subject font-green bold uppercase">Liste des utilisateurs</span>                    </div>                    <div class="actions">                        <div class="btn-group">                            <a class="btn btn-circle btn-default btn-sm" href="javascript:;" data-toggle="dropdown">                                <i class="fa fa-cog"></i>                                <i class="fa fa-angle-down"></i>                            </a>                            <ul class="dropdown-menu" role="menu">                                <li>                                    <a href="javascript:;">                                        <i class="icon-movie"></i> Créer une séance </a>                                </li>                                <li>                                    <a href="javascript:;">                                        <i class="icon-present"></i> New Event                                        <span class="badge badge-success">4</span>                                    </a>                                </li>                                <li>                                    <a href="javascript:;">                                        <i class="icon-basket"></i> New order </a>                                </li>                                <li class="divider"></li>                                <li>                                    <a href="javascript:;">                                        <i class="icon-flag"></i> Pending Orders                                        <span class="badge badge-danger">4</span>                                    </a>                                </li>                                <li>                                    <a href="javascript:;">                                        <i class="icon-users"></i> Pending Users                                        <span class="badge badge-warning">12</span>                                    </a>                                </li>                            </ul>                        </div>                        {#<a class="btn btn-circle btn-icon-only btn-default" href="javascript:;">#}                        {#<i class="icon-cloud-upload"></i>#}                        {#</a>#}                        {#<a class="btn btn-circle btn-icon-only btn-default" href="javascript:;">#}                        {#<i class="icon-wrench"></i>#}                        {#</a>#}                        {#<a class="btn btn-circle btn-icon-only btn-default" href="javascript:;">#}                        {#<i class="icon-trash"></i>#}                        {#</a>#}                        <a class="btn btn-circle btn-icon-only btn-default fullscreen" href="javascript:;"                           data-original-title="" title=""> </a>                    </div>                </div>                <div class="portlet-body">                    <div class="row">                        <div class="col-sm-10">                            {#{{ render(controller("Zone51AdsBundle:Ads:searchAds")) }}#}                            {#{% include 'Zone51AdsBundle:Ads:search.html.twig' %}#}                        </div>                        <div class="col-sm-12">                            {# total items count #}                            <div class="count">                                <h5> Nombre d'annonces:                                    {% if sessions %} {{ sessions.getTotalItemCount }} {% else %} 0 {% endif %}                                </h5>                            </div>                            <div class="alert alert-success hide response-ajax">                                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span                                            aria-hidden="true">&times;</span></button>                                <p class="text-response-ajax"></p>                            </div>                            <div class="table-responsive">                                <table class="table table-hover">                                    {% if sessions is defined %}                                        <tr class="">                                            {# sorting of properties based on query components #}                                            <th>ID</th>                                            <th>Titre</th>                                            <th>Créateur</th>                                            <th>Prix</th>                                            <th>Date</th>                                            <th>Nbr Place(s)</th>                                            <th class="col-md-2">Actions</th>                                        </tr>                                        {% for session in sessions %}                                            <tr {% if loop.index is odd %}class="color"{% endif %}                                                id="tr-session-{{ session.id }}">                                                <td>{{ session.id }}</td>                                                <td>{{ session.film.title }}</td>                                                <td>{{ session.creator.username | capitalize }}</td>                                                <td>{{ session.price }}</td>                                                <td>{{ session.date| date("d M") }} à                                                    {{ session.date| date("H") }}h{{ session.date| date("i") }}</td>                                                <td>{{ session.maxPlace }}</td>                                                <td>                                                    <a href="#"                                                       class="btn btn-default btn-xs">                                                        <i class="glyphicon glyphicon-eye-open text-success"></i>                                                    </a>                                                    <a href="#"                                                       class="btn btn-default btn-xs">                                                        <i class="glyphicon glyphicon-cog text-primary"></i>                                                    </a>                                                    <a href="#"                                                       class="btn btn-default btn-xs btn-remove"                                                       data-toggle="modal"                                                       data-action="session"                                                       data-value="session-{{ session.id }}"                                                       data-line="tr-session-{{ session.id }}">                                                        <i class="glyphicon glyphicon-trash text-danger "                                                           data-target="#myModal"></i>                                                    </a>                                                </td>                                            </tr>                                        {% endfor %}                                    {% else %}                                        <tr>                                            <td colspan="8" class="active"><h3 class="text-center ">Aucune annonce                                                    correspondante à                                                    vos recherche</h3></td>                                        </tr>                                    {% endif %}                                </table>                            </div>                            <div class="navigation">                                {% if sessions %}                                    {{ knp_pagination_render(sessions) }}                                {% endif %}                            </div>                        </div>                    </div>                </div>            </div>            <!-- END SAMPLE TABLE PORTLET-->        </div>    </div>                {% endblock %}    {% block javascripts %}        {{ parent() }}        {% javascripts        "bundles/mc/themes/global/scripts/datatable.js"        "bundles/mc/themes/global/plugins/datatables/datatables.min.js"        "bundles/mc/themes/global/plugins/datatables/plugins/bootstrap/datatables.bootstrap.js"        "bundles/mc/themes/global/scripts/app.min.js"        "bundles/mc/themes/pages/scripts/table-datatables-fixedheader.min.js"         "bundles/mc/js/applications.js"        %}        <script src="{{ asset_url }}"></script>        {% endjavascripts %}    {% endblock %}