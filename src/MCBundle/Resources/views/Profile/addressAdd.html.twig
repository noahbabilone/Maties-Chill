{% extends "MCBundle::main.html.twig" %}{% block title %}    Films - {{ parent() }}{% endblock %}{% block stylesheets %}    {{ parent() }}    {% stylesheets    filter='cssrewrite' %}    <link rel="stylesheet" href="{{ asset_url }}"/>    {% endstylesheets %}{% endblock %}{% block body_mc %}    <div class="page-container page-mc">        <div class="page-content">            <div class="page-bar">                <div class="row">                    <div class="col-sm-12">                        <ul class="page-breadcrumb">                            <li>                                <a href="{{ path('home_page') }}">Accueil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <a href="{{ path('fos_user_profile_show') }}">Profil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <a href="{{ path('user_address') }}">Mes Adresses</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <span>Ajoute</span>                            </li>                        </ul>                    </div>                </div>            </div>            <!-- END PAGE BAR -->            <!-- BEGIN PAGE TITLE-->            <div class="row">                <div class="col-sm-2">                    {% include("MCBundle:Includes:submenu.session.html.twig") %}                </div>                <div class="col-sm-10">                    <div class="row">                        <div class="col-sm-12">                            <h3 class="page-title page-header">                                Profile                                <small> Ajout adresse</small>                            </h3>                        </div>                        {% if form is defined %}                            {{ form_start(form, { 'method': 'POST','attr': { 'role':'form'} }) }}                            <div class="col-sm-8 col-sm-offset-2">                                <div class="form-body">                                    <div class="row">                                                {{ form_errors(form) }}                                        {% if form.title is defined %}                                            <div class="col-sm-12">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.title) }}                                                    <div class="input-icon">                                                        <i class="fa fa-home"></i>                                                        {{ form_widget(form.title) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.street is defined %}                                            <div class="col-sm-4">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.street) }}                                                    <div class="input-icon">                                                        <i class="fa fa-road"></i>                                                        {{ form_widget(form.street) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.address is defined %}                                            <div class="col-sm-8">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.address) }}                                                    <div class="input-icon">                                                        <i class="fa fa-map-marker"></i>                                                        {{ form_widget(form.address) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.postCode is defined %}                                            <div class="col-sm-4">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.postCode) }}                                                    <div class="input-icon">                                                        <i class="fa fa-globe"></i>                                                        {{ form_widget(form.postCode) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.town is defined %}                                            <div class="col-sm-8">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.town) }}                                                    <div class="input-icon">                                                        <i class="fa fa-location-arrow"></i>                                                        {{ form_widget(form.town) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.frontDoor is defined %}                                            <div class="col-sm-4">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.frontDoor) }}                                                    <div class="input-icon">                                                        <i class="fa fa-sign-in"></i>                                                        {{ form_widget(form.frontDoor) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.floor is defined %}                                            <div class="col-sm-4">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.floor) }}                                                    <div class="input-icon">                                                        <i class="fa fa-building"></i>                                                        {{ form_widget(form.floor) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.building is defined %}                                            <div class="col-sm-4">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.building) }}                                                    <div class="input-icon">                                                        <i class="fa fa-building-o"></i>                                                        {{ form_widget(form.building) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                        {% if form.phone is defined %}                                            <div class="col-sm-6">                                                <div class="form-group has-feedback" id="#">                                                    {{ form_label(form.phone) }}                                                    <div class="input-icon">                                                        <i class="fa fa-phone"></i>                                                        {{ form_widget(form.phone) }}                                                    </div>                                                </div>                                            </div>                                        {% endif %}                                    </div>                                </div>                            </div>                            <div class="col-sm-12"></div>                            <div class="form-actions">                                <div class="row">                                    <div class="col-sm-3 col-sm-offset-2">                                        <button type="submit" class="btn green btn-block btn-add-user">Envoyer</button>                                    </div>                                    <div class="col-sm-3">                                        <button type="button" class="btn default btn-block">Annuler</button>                                    </div>                                </div>                            </div>                            {{ form_end(form) }}                        {% endif %}                    </div>                </div>            </div>        </div>    </div>{% endblock %}   {% block javascripts %}       {{ parent() }}       {% javascripts       'bundles/mc/js/applications.js' %}       {#  #}       <script src="{{ asset_url }}"></script>       {% endjavascripts %}   {% endblock %}