{% extends "MCBundle::main.html.twig" %}{% block title %}    Films - {{ parent() }}{% endblock %}{% block stylesheets %}    {{ parent() }}    {% stylesheets    filter='cssrewrite' %}    <link rel="stylesheet" href="{{ asset_url }}"/>    {% endstylesheets %}{% endblock %}{% block body_mc %}    <div class="page-container page-mc">        <div class="page-content">            <div class="page-bar">                <div class="row">                    <div class="col-sm-12">                        <ul class="page-breadcrumb">                            <li>                                <a href="{{ path('home_page') }}">Accueil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <a href="{{ path('fos_user_profile_show') }}">Profil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <span>Séances</span>                            </li>                        </ul>                    </div>                </div>            </div>            <!-- END PAGE BAR -->            <!-- BEGIN PAGE TITLE-->            <div class="row">                <div class="col-sm-2">                    {% include("MCBundle:Includes:submenu.seance.html.twig") %}                </div>                <div class="col-sm-10">                    <div class="row">                        <div class="col-sm-12">                            <h3 class="page-title page-header">                                Profile                                <small> Séances</small>                            </h3>                        </div>                        {% if results is defined %}                            <div class="col-sm-12">                                {#{{ dump(seances) }}#}                                <div class="table-scrollable table-scrollable-borderless">                                    <table class="table table-hover table-light">                                        <thead>                                        <tr>                                            <th>Films</th>                                            <th>Prix</th>                                            <th>Place(s)</th>                                            <th>Type View</th>                                            <th>Participant</th>                                            <th>Date</th>                                            <th>Actions</th>                                        </tr>                                        </thead>                                        <tbody>                                        {% for result in results %}                                            <tr id="tr-seance-{{ result.seance.id }}">                                                <td class="col-sm-5 td-img">                                                    {% if result.seance.film.poster %}                                                        <img src="{{ result.seance.film.poster }}" alt="">                                                    {% else %}                                                        <img src="{{ asset('assets/img/default.png') }}" alt="">                                                    {% endif %}                                                    {{ result.seance.film.title | capitalize }}                                                </td>                                                <td class="col-sm-1">{{ result.seance.price }}€</td>                                                <td class="col-sm-1">{{ result.seance.maxPlace }}</td>                                                <td class="col-sm-1">{{ result.seance.typeView | upper }}</td>                                                <td class="col-sm-1">{{ result.participants }}</td>                                                <td class="col-sm-2">{{ result.seance.date| date('d F Y') }}</td>                                                <td class="col-sm-2">                                                    <a href="{{ path(app.request.attributes.get('_route'),                                                    app.request.attributes.get('_route_params')  |merge(app.request.query.all)|                                                    merge({'view': result.seance.id })) }}"                                                       class="font-green-jungle fa-fw"><i class="fa fa-eye"></i></a>                                                    <a href="#" class="font-edit"><i class="fa fa-edit fa-fw"></i></a>                                                    <a href="javascript:;" class="font-red btn-remove"                                                       data-toggle="modal"                                                       data-action="seance"                                                       data-title="{{ result.seance.film.title }} - {{ result.seance.typeView  | upper }}"                                                       data-value="seance-{{ result.seance.id }}"                                                       data-line="tr-seance-{{ result.seance.id }}"><i                                                                class="fa fa-trash"></i></a>                                                </td>                                            </tr>                                        {% endfor %}                                        </tbody>                                    </table>                                </div>                                <div class="cbp-item">                                    {% if results %}                                        {{ knp_pagination_render(results) }}                                    {% endif %}                                </div>                            </div>                        {% endif %}                    </div>                </div>            </div>        </div>    </div>{% endblock %}   {% block javascripts %}       {{ parent() }}       {% javascripts       'bundles/mc/js/applications.js' %}       <script src="{{ asset_url }}"></script>       {% endjavascripts %}   {% endblock %}   