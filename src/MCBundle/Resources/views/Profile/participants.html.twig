{% extends "MCBundle::main.html.twig" %}{% block title %}    Films - {{ parent() }}{% endblock %}{% block stylesheets %}    {{ parent() }}    {% stylesheets    'bundles/mc/css/jquery.rateyo.css'    'bundles/mc/css/profil.css'    filter='cssrewrite' %}    <link rel="stylesheet" href="{{ asset_url }}"/>    {% endstylesheets %}    <style type="text/css">    </style>{% endblock %}{% block body_mc %}    <div class="page-container page-mc">        <div class="page-content">            <div class="page-bar">                <div class="row">                    <div class="col-sm-12">                        <ul class="page-breadcrumb">                            <li>                                <a href="{{ path('home_page') }}">Accueil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <a href="{{ path('fos_user_profile_show') }}">Profil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <span>Participants</span>                            </li>                        </ul>                    </div>                </div>            </div>            <!-- END PAGE BAR -->            <!-- BEGIN PAGE TITLE-->            <div class="row">                <div class="col-sm-2">                    {% include("MCBundle:Includes:submenu.session.html.twig") %}                </div>                <div class="col-sm-10">                    <div class="row">                        <div class="col-sm-12">                            <h3 class="page-title page-header">                                Profile                                <small> Participants</small>                            </h3>                        </div>                        {% if participants is defined %}                            {#{{ dump(participants) }}#}                            <div class="col-sm-12" id="participants">                                {#{{ dump(sessions) }}#}                                <div class="table-scrollable table-scrollable-borderless">                                    <table class="table table-hover table-light">                                        <thead>                                        <tr>                                            <th>Participants</th>                                            <th>Séances</th>                                            <th>Date</th>                                            <th>Actions</th>                                        </tr>                                        </thead>                                        <tbody>                                        {% for participant in participants %}                                            <tr id="tr-session-{{ participant.user.username }}-{{ loop.index }} ">                                                <td class="col-sm-2">                                                    <a href="javascript:;"                                                       class="view-profil"                                                       data-username="{{ participant.user.username | capitalize }}"                                                       data-id-user="{{ participant.user.id }}"                                                       data-rating="{{ random(5) }}"                                                       title="voir profil {{ participant.user.username | capitalize }}">{{ participant.user.username | capitalize }}                                                    </a>                                                </td>                                                <td class="col-sm-7 td-img">                                                    {% if participant.session.film.poster is defined and participant.session.film.poster %}                                                        <img src="{{ participant.session.film.poster }}" alt="">                                                    {% else %}                                                        <img src="{{ asset('assets/img/default.png') }}" alt="">                                                    {% endif %}                                                    {{ participant.session.film.title | capitalize }}                                                    - {{ participant.session.typeView  |upper }}                                                    - {{ participant.session.creator.username | capitalize }}  </td>                                                <td class="col-sm-1">{{ participant.session.date | date ('d/m/y') }}</td>                                                {#<td>{{ participant.participant | length }}</td>#}                                                {#<td>{{ participant.date| date('d F Y') }}</td>#}                                                <td class="col-sm-4">                                                                                                        <a href="javascript:;" class="blue btn btn-xs"                                                       data-toggle="modal"                                                       data-action="profil"                                                       data-id-user="{{ participant.user.id }}"                                                    ><i class="fa fa-envelope"></i> Message</a>                                                    <a href="javascript:;" class="red btn btn-xs"                                                       data-toggle="modal"                                                       data-action="profil"                                                       data-id-user="{{ participant.user.id }}"                                                    ><i                                                                class="fa fa-trash"></i> Enlever</a>                                                </td>                                                                                              </tr>                                        {% endfor %}                                        </tbody>                                    </table>                                </div>                                <div class="cbp-item">                                    {% if participants %}                                        {{ knp_pagination_render(participants) }}                                    {% endif %}                                </div>                            </div>                        {% endif %}                    </div>                </div>            </div>        </div>    </div>    <div class="modal fade" tabindex="-1" id="profil-modal" role="dialog"         data-attention-animation="false">        <div class="modal-dialog" role="document">            <!-- Modal content-->            <div class="modal-content">                {#<div class="modal-header">#}                {#</div>#}                <div class="modal-body">                    <div class="row profile">                        <div class="col-md-12">                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span                                        aria-hidden="true">&times;</span></button>                            <div class="profile-sidebar">                                <!-- SIDEBAR USERPIC -->                                <div class="profile-userpic">                                    <img src="{{ asset('assets/img/profile.png') }}"                                         class="img-responsive" alt="">                                </div>                                <!-- END SIDEBAR USERPIC -->                                <!-- SIDEBAR USER TITLE -->                                <div class="profile-usertitle">                                    <div class="profile-usertitle-name">                                        Marcus Doe                                    </div>                                    <div class="col-sm-offset-5">                                        <span id="profileUserRating"></span>                                    </div>                                </div>                                <!-- END SIDEBAR USER TITLE -->                                <!-- SIDEBAR BUTTONS -->                                <div class="profile-userbuttons">                                    <button type="button" class="btn btn-success btn-sm"><i class="fa fa-envelope"></i>                                        Message                                    </button>                                    <button type="button" class="btn yellow-casablanca btn-sm"><i                                                class="fa fa-star"></i><i class="fa fa-star-o"></i><i                                                class="fa fa-star-o"></i> Noter                                    </button>                                </div>                                <!-- END SIDEBAR BUTTONS -->                                <!-- SIDEBAR MENU -->                                <div class="profile-usermenu">                                    <ul class="nav">                                        <li class="">                                            <a href="#"><i class="fa fa-tv"></i> Séances</a>                                        </li>                                        <li>                                            <a href="#"><i class="glyphicon glyphicon-user"></i> Appréciations</a>                                        </li>                                    </ul>                                </div>                                <!-- END MENU -->                            </div>                        </div>                    </div>                </div>            </div>        </div>    </div>{% endblock %}   {% block javascripts %}       {{ parent() }}       {% javascripts       'bundles/mc/js/jquery.rateyo.min.js'       'bundles/mc/js/applications.js' %}       <script src="{{ asset_url }}"></script>       {% endjavascripts %}   {% endblock %}   