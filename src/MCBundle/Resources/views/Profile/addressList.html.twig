{% extends "MCBundle::main.html.twig" %}{% block title %}    Films - {{ parent() }}{% endblock %}{% block stylesheets %}    {{ parent() }}    {% stylesheets    filter='cssrewrite' %}    <link rel="stylesheet" href="{{ asset_url }}"/>    {% endstylesheets %}{% endblock %}{% block body_mc %}    <div class="page-container page-mc">        <div class="page-content">            <div class="page-bar">                <div class="row">                    <div class="col-sm-12">                        <ul class="page-breadcrumb">                            <li>                                <a href="{{ path('home_page') }}">Accueil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <a href="{{ path('fos_user_profile_show') }}">Profil</a>                                <i class="fa fa-circle"></i>                            </li>                            <li>                                <span>Adresse</span>                            </li>                        </ul>                    </div>                </div>            </div>            <!-- END PAGE BAR -->            <!-- BEGIN PAGE TITLE-->            <div class="row">                <div class="col-sm-2">                    {% include("MCBundle:Includes:submenu.session.html.twig") %}                </div>                <div class="col-sm-10">                    <div class="row">                        <div class="col-sm-12">                            <h3 class="page-title page-header">                                Profile                                <small> Adresse</small>                            </h3>                        </div>                        {% if addresses %}                            {% for address in addresses %}                                {#{{ dump(address) }}#}                                <div class="col-sm-4" id="div-address-{{ address.id }}">                                    <div class="table-scrollable table-scrollable-borderless">                                        <table class="table table-hover table-light">                                            <tr>                                                <td colspan="2">                                                    {{ address.title | upper }}                                                    <span class="pull-right">                                             <a href="" class=""><i class="fa fa-edit font-blue-soft"></i></a>                                            <a href="" class="btn-remove"                                               data-action="address"                                               data-title=" {{ address.title | upper }}"                                               data-value="address-{{ address.id }}"                                               data-line="div-address-{{ address.id }}"><i                                                        class="fa fa-trash font-red"></i></a>                                            </span>                                                </td>                                            </tr>                                            <tr>                                                <td>Adresse:</td>                                                <td>                                                    {{ address.street }} {{ address.address }}                                                 </td>                                            </tr>                                            <tr>                                                <td> Code postal:</td>                                                <td> {{ address.postCode }}</td>                                            </tr>                                            <tr>                                                <td> Ville:</td>                                                <td> {{ address.town }}  </td>                                            </tr>                                            <tr>                                                <td> Pays:</td>                                                <td> FRANCE</td>                                            </tr>                                            <tr>                                                <td> TÃ©l:</td>                                                <td> {{ address.phone }} </td>                                            </tr>                                            <tr>                                                <td> {{ address.other }}</td>                                            </tr>                                        </table>                                    </div>                                </div>                            {% endfor %}                            <div class="col-sm-12 cbp-item">                                {{ knp_pagination_render(addresses) }}                            </div>                        {% endif %}                    </div>                </div>            </div>        </div>    </div>{% endblock %}   {% block javascripts %}       {{ parent() }}       {% javascripts       'bundles/mc/js/applications.js' %}       {#  #}       <script src="{{ asset_url }}"></script>       {% endjavascripts %}   {% endblock %}